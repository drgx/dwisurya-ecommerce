
	{{ header }}
	<div id="common-home">    
	    {% if column_left and column_right %}
	    {% set class = 'col-sm-6' %}
	    {% elseif column_left or column_right %}
	    {% set class = 'col-sm-9' %}
	    {% else %}
	    {% set class = 'col-sm-12' %}
	    {% endif %}    
	    {{ content_top }}
	    <div class="content_middle">
		    <div class="container">
		    	<div class="row">
		    		{{ content_middle }}
		    	</div>
		    </div>
		</div>
		{{ content_bottom }}
	    {{ column_left }} 
	    {{ column_right }}
	</div>
	{{ footer }}   